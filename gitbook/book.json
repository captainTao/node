{
  "root": "./",
  "title": "技术笔记文档",
  "gitbook": "3.2.3",
  "author": "Captain",
  "authorHomepage": "https://blog.csdn.net/Iron_Ye",
  "description": "",
  "language": "zh-hans",
  "copyright": "All Rights Reserved",
  "output.name": "site",
  "styles": {
    "website": "styles/website.css",
    "ebook": "styles/ebook.css",
    "pdf": "styles/pdf.css",
    "mobi": "styles/mobi.css",
    "epub": "styles/epub.css"
  },
  "plugins": [
    "-search",
    "-share",
    "-lunr",
    "-highlight",
    "-search-pro",
    "-expandable-chapters-small-hasbug",
    "code",
    "search-plus",
    "anchor-navigation-ex",
    "splitter",
    "tbfed-pagefooter"
  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
            "copyright":"Copyright &copy Camera360 2019",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
      "showLevel": true
    },
    "anchor-navigation-ex": {
      "showLevel": true,
      "associatedWithSummary": false,
      "printLog": false,
      "multipleH1": false,
      "mode": "float",
      "float": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    }
  }
}